{
  "name": "ublacklist",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node scripts/build.ts",
    "check": "pnpm run /^check:/",
    "check:biome": "biome check",
    "check:prettier": "prettier --check **/*.{html,md,yaml,yml}",
    "check:tsc": "tsc --build",
    "fix": "pnpm run /^fix:/",
    "fix:biome": "biome check --write",
    "fix:prettier": "prettier --write **/*.{html,md,yaml,yml}",
    "generate": "pnpm run /^generate:/",
    "generate:message-names": "node scripts/generate-message-names.ts",
    "generate:ruleset-parser": "lezer-generator src/scripts/ruleset/ruleset.grammar -o src/scripts/ruleset/parser.generated.ts --noTerms --typeScript",
    "generate:third-party-notices": "node scripts/generate-third-party-notices.ts",
    "test": "node scripts/test.ts"
  },
  "dependencies": {
    "@codemirror/commands": "^6.9.0",
    "@codemirror/lang-yaml": "^6.1.2",
    "@codemirror/language": "^6.11.3",
    "@codemirror/lint": "^6.9.0",
    "@codemirror/state": "^6.5.2",
    "@codemirror/view": "^6.38.6",
    "@lezer/common": "^1.3.0",
    "@lezer/highlight": "^1.2.2",
    "@lezer/lr": "^1.4.2",
    "@mdi/svg": "^7.4.47",
    "@neodrag/vanilla": "^2.3.1",
    "colord": "^2.9.3",
    "css-tree": "^3.1.0",
    "dayjs": "^1.11.18",
    "es-toolkit": "^1.40.0",
    "goober": "2.1.15",
    "is-mobile": "^5.0.0",
    "js-yaml": "^4.1.0",
    "punycode": "^2.3.1",
    "react": "^19.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.2.0",
    "tldts": "^7.0.17",
    "zod": "^4.1.12",
    "zod-validation-error": "4.0.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@lezer/generator": "^1.8.0",
    "@types/css-tree": "^2.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/license-checker": "^25.0.6",
    "@types/node": "^24.8.1",
    "@types/punycode": "^2.1.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webextension-polyfill": "^0.12.4",
    "csstype": "^3.1.3",
    "dotenv": "^17.2.3",
    "esbuild": "^0.25.11",
    "fs-extra": "^11.3.2",
    "lefthook": "^2.0.0",
    "license-checker": "^25.0.1",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.1",
    "semantic-release-amo": "^2.0.1",
    "semantic-release-chrome": "^3.2.0",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.19.0",
  "engines": {
    "pnpm": ">=10"
  }
}
